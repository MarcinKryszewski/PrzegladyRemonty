<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Grey300Color">#131313</Color>
    <Color x:Key="Grey400Color">#191919</Color>
    <Color x:Key="Grey500Color">#272822</Color>
    <Color x:Key="Grey600Color">#333333</Color>
    <Color x:Key="Grey700Color">#545454</Color>

    <Color x:Key="MainDarkColor">#890019</Color>
    <Color x:Key="MainColor">#c50024</Color>
    <Color x:Key="MainLightColor">#D64C65</Color>

    <Color x:Key="WhiteDarkColor">#cfcfcf</Color>
    <Color x:Key="WhiteLightColor">#d3d3d3</Color>

    <SolidColorBrush x:Key="Grey300Brush">#131313</SolidColorBrush>
    <SolidColorBrush x:Key="Grey400Brush">#191919</SolidColorBrush>
    <SolidColorBrush x:Key="Grey500Brush">#272822</SolidColorBrush>
    <SolidColorBrush x:Key="Grey600Brush">#333333</SolidColorBrush>
    <SolidColorBrush x:Key="Grey700Brush">#545454</SolidColorBrush>

    <SolidColorBrush x:Key="MainDarkBrush">#890019</SolidColorBrush>
